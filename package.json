{
  "name": "akroma",
  "version": "0.0.5",
  "description": "Akroma Desktop Wallet",
  "homepage": "https://github.com/akroma-project/akroma-wallet-desktop",
  "author": {
    "name": "Ian DesJardins",
    "email": "ian.desjardins@akroma.io"
  },
  "keywords": [
    "angular",
    "angular 6",
    "angular-cli 6",
    "electron 2",
    "typescript",
    "sass"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "node postinstall.js && npx electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p ng:serve  electron:serve",
    "postinstall:electron": "node postinstall",
    "build": "npm run electron:tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "electron:tsc": "tsc main.ts",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "test": "ng test",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "bignumber.js": "^7.0.1",
    "crypto-browserify": "^3.12.0",
    "install": "^0.11.0",
    "ng2-search-filter": "^0.4.7",
    "ngx-bootstrap": "^2.0.5",
    "ngx-pagination": "^3.1.1",
    "ngx-qrcode2": "0.0.9",
    "node-pre-gyp": "^0.10.0",
    "npm": "^6.0.1",
    "pouchdb": "^6.4.3",
    "pouchdb-adapter-node-websql": "^6.4.3",
    "pouchdb-browser": "^6.4.3",
    "request": "^2.85.0",
    "rxjs-compat": "^6.2.0",
    "web3": "^1.0.0-beta.34",
    "websocket": "^1.0.26",
    "websql": "^0.4.4"
  },
  "devDependencies": {
    "@angular/cli": "6.0.8",
    "@angular/common": "6.0.9",
    "@angular/compiler": "6.0.9",
    "@angular/compiler-cli": "6.0.9",
    "@angular/core": "6.0.9",
    "@angular/forms": "6.0.9",
    "@angular/http": "6.0.9",
    "@angular/language-service": "6.0.9",
    "@angular/platform-browser": "6.0.9",
    "@angular/platform-browser-dynamic": "6.0.9",
    "@angular/router": "6.0.9",
    "@angular-devkit/build-angular": "~0.6.5",
    "@ngx-translate/core": "10.0.1",
    "@ngx-translate/http-loader": "3.0.1",
    "@types/jasmine": "2.8.7",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.9.4",
    "@types/pouchdb": "^6.3.2",
    "@types/pouchdb-browser": "^6.1.2",
    "codelyzer": "4.2.1",
    "core-js": "2.5.6",
    "cross-env": "5.1.4",
    "dotenv": "5.0.1",
    "electron": "2.0.5",
    "electron-builder": "20.20.4",
    "electron-reload": "1.2.5",
    "fs-extra": "5.0.0",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.0",
    "karma-electron-launcher": "^0.2.0",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.1.0",
    "npm-run-all": "4.1.3",
    "npx": "10.2.0",
    "protractor": "5.3.2",
    "replace": "0.3.0",
    "rxjs": "6.1.0",
    "ts-node": "6.0.3",
    "tslint": "5.10.0",
    "typescript": "2.7.2",
    "wait-on": "2.1.0",
    "webdriver-manager": "12.0.6",
    "zone.js": "0.8.26"
  }
}